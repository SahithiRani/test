#program to find largest element in the array of integers 
int n,i=0; 
print("Enter the size of the array:"); 
n=scan(); 
int arr[n]; 
print("Enter "+n+"Elements : \n"); 
loop(i<n) 
{
    arr[i]=scan(); 
    i=i+1; 
}
int max=arr[0]; 
i=0; 
loop (i<n) 
{
    if(arr[i]>max) 
    {
        max=arr[i];
    }
    i=i+1; 
}
print("Largest element: "+max);